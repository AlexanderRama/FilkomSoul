<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Filkomsoul"
        tools:targetApi="31">

        <activity
            android:name=".ui.WebView"
            android:exported="false" />
        <activity
            android:name=".ui.screening.result.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".ui.screening.questionnaire.QuestionnaireActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.article.NewsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.AboutAppActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.archive.ArchiveActivity"
            android:exported="false" />
        <activity
            android:name=".ui.auth.RegistActivity"
            android:exported="false" />
        <activity
            android:name=".ui.auth.OnboardActivity"
            android:exported="false" />
        <activity
            android:name=".ui.auth.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:label="@string/app_name" />
    </application>
    <queries>
        <package android:name="com.whatsapp" />
    </queries>
</manifest>